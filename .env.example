# Canvas MCP Server Configuration
# Copy this file to .env and customize as needed

# HTTP server port (default: 8765)
CANVAS_MCP_PORT=8765

# Custom log file location (default: ~/canvas-mcp-host.log)
# CANVAS_MCP_LOG=/path/to/custom/logfile.log

# Enable verbose logging (default: false)
# CANVAS_MCP_VERBOSE=true

# CORS allowed origins (comma-separated, default: *)
# CANVAS_MCP_CORS_ORIGINS=http://localhost:8765,https://example.com
